{
  "rewrites": [
    {
      "source": "/ar/:path*",
      "destination": "/api/ar?url=:path*"
    }
  ],
  "headers": [
    {
      "source": "/api/ar",
      "headers": [
        {
          "key": "Content-Type",
          "value": "model/vnd.usdz+zip"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000"
        }
      ]
    }
  ]
}
